#contact
  %h1.upcase
    %span.slash #
    contact
    -# %span.slash &#10095;
  = form_for Object.new, 'http://localhost:3030/users' do |f|
    = f.text_field :name, placeholder: "Your Name"
    .input-border
    = f.text_field :email, placeholder: "Your Email"
    .input-border
    %textarea{ placeholder: "Your Message" }
    .input-border
    %button{ type: :submit, class: "submit"}
      %i.fa.fa-paper-plane-o
  .social
    = link_to "https://github.com/npezza93", class: "social-container" do
      %i.fa.fa-github
    = link_to "https://www.linkedin.com/in/nick-pezza-02166668", class: "social-container" do
      %i.fa.fa-linkedin
